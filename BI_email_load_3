/* ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
4-digit rank table that can left join to show missing rank groups also.
Plus pick list for US states
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ */
with digits as (
      select substring('1234', 1, 1) as d union all
      select substring('1234', 2, 1) as d union all
      select substring('1234', 3, 1) as d union all
      select substring('1234', 4, 1) as d 
     )
select (d1.d + d2.d + d3.d + d4.d) as ranking
from digits d1 cross join
     digits d2 cross join 
     digits d3 cross join
     digits d4
order by ranking desc

create table digit4 (ranking int);
-- select * from digit4;

